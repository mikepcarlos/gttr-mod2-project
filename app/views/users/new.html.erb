<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :bio %>
  <%= f.text_field :bio%>
  <%= f.label :display_name %>
  <%= f.text_field :display_name %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation  %>
  <%= f.submit %>
<% end %>
